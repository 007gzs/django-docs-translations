msgid "Port 7000 on IP address ``127.0.0.1``::"
msgstr "Port 7000 sur l'adresse IP ``127.0.0.1``::"

#: ../../../../sources/1.9/docs/ref/django-admin.txt:877
msgid "Port 7000 on IP address ``1.2.3.4``::"
msgstr "Port 7000 sur l'adresse IP ``1.2.3.4``::"

#: ../../../../sources/1.9/docs/ref/django-admin.txt:881
msgid "Port 8000 on IPv6 address ``::1``::"
msgstr "Port 8000 sur l'adresse IPv6 ``::1``::"

