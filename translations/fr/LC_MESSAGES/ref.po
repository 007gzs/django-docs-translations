msgstr "``app_index.html``"

# 47a54f2dd76e4b708d8b10d415aacf79
#: ../../../../1.7/docs/ref/contrib/admin/index.txt:2390
msgid "``change_form.html``"
msgstr "``change_form.html``"

# 15ee56f0715644c7a73377cc43bab5c8
#: ../../../../1.7/docs/ref/contrib/admin/index.txt:2391
msgid "``change_list.html``"
msgstr "``change_list.html``"

# 2ad1e8260d2a4ee297b75176f1d0f6a6
