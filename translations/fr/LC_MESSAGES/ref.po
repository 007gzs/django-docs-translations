# 7d6d55f2088244e085de4c1950ae955e
#: ../../../../1.6/docs/ref/files/file.txt:121
msgid "Width of the image in pixels."
msgstr "Largeur de l'image en pixels."

# 10fdf7a34f2541fcb778d5e250dd259c
#: ../../../../1.6/docs/ref/files/file.txt:125
