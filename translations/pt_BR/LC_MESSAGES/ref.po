msgid "``ETag``"
msgstr ""

#: ../../../../sources/1.9/docs/ref/utils.txt:57
msgid "``Last-Modified``"
msgstr ""

#: ../../../../sources/1.9/docs/ref/utils.txt:58
msgid "``Expires``"
msgstr ""

#: ../../../../sources/1.9/docs/ref/utils.txt:59
msgid "``Cache-Control``"
